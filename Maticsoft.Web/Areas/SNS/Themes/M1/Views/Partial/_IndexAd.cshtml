
@model List<Maticsoft.Model.Settings.Advertisement>
<script src="/Areas/SNS/Themes/M1/Content/Scripts/IndexBannerAD.js" type="text/javascript"></script>
<style type="text/css">
ul,li{ margin:0px; padding:0px;}
#focus {width:684px; height:263px; overflow:hidden; position:relative;}
#focus ul {height:380px; position:absolute;}
#focus ul li {float:left; width:684px; height:263px; overflow:hidden; position:relative; background:#000;}
#focus ul li div {position:absolute; overflow:hidden;}
#focus .btnBg {position:absolute; width:684px; height:20px; left:0; bottom:0; background:#000;}
#focus .btn {position:absolute; width:780px; height:10px; padding:5px 10px; right:0; bottom:0; text-align:right;}
#focus .btn span {display:inline-block; _display:inline; _zoom:1; width:25px; height:10px; _font-size:0; margin-left:5px; cursor:pointer; background:#fff;}
#focus .btn span.on {background:#fff;}
#focus .preNext {width:45px; height:100px; position:absolute; top:70px; background: url(/Areas/SNS/Themes/M1/Content/images/sprite.png) no-repeat 0 0; cursor:pointer;}
#focus .pre {left:0;}
#focus .next {right:0; background-position:right top;}
.btn {
-webkit-border-radius: 4px;
border-radius: 4px;
cursor: pointer;
text-align: center;
background-color: #F69;
color: white;
float: right;
height: 20px;
line-height: 19px;
width: 64px;
}
</style>
<div id="focus">
    @if (Model != null && Model.Count > 0)
    {
        <ul>
        @foreach (var item in Model)
        {
            <li><a target="_blank" href="@item.NavigateUrl">
                   <img border="0" src="@item.FileUrl"  width="684" height="263"  ></a></li>
        }
    </ul>
    }p
    
</div> 