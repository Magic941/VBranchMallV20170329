.Track{
	width::95%;
	margin:0 auto;
	border:1px solid #b2b2b2;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	}
.Track_top{
	background:#ffd2da;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
	padding:1.5rem 0;
	border-bottom:1px solid #b2b2b2;
	}
.Track_top p{
	margin:0;
	color:#d91047;
	padding:0 1rem;
	overflow:hidden;
	}
.Track_top p em,.Track_top p i{
	font-style:normal;
	font-size:0.9rem;
	}
	
.Track_top p em{
	width:5rem;
	float:left;
	display:block;
	text-align:right;
	
	}
.Track_message{
	border-left:1px solid #d91047;
	margin-top:10px;
	padding-left:10px;
	margin-bottom:30px;
	}
.Track_message p{
	color:#666666;
	}
.Track_message p,.Track_message span{
	font-size:0.8rem;
	}
.Track_message span{
	color:#5dd0ab;
	}
.Track_message li{
	position:relative;
	padding-bottom:30px;
	}
.Track_message_top em{
	width:1rem;
	height:1.5rem;
	display:block;
	position:absolute;
	left:-19px;
	top:0;
	}
.Track_message_top em img{
	width:100%;
	}
.Track_message li i{
	position:absolute;
	width:0.8rem;
	height:0.8rem;
	background:#d91047;
	display:block;
	border-radius:50%;
	top:10px;
	left:-17px;
	}
</style>


<div class="top">
    <a href="javascript:;" onclick="history.go(-1)"><span>&lt;返回</span></a>
    @Html.Partial("/Areas/MShop/Themes/M1/Views/Partial/_HeadRight.cshtml")
</div>
<div class="Track">
	<div class="Track_top">
        <p><em>配送方式：</em>  <i>@ViewBag.EName</i></p>
        <p><em>快递单号：</em> <i> @ViewBag.Code</i></p>
    </div>
    <div class="" style="padding-left:10px;">
        <div class="Track_message">
            <ul>
                @{
                    bool tag = true;
                    foreach (Maticsoft.Model.Shop.Shipping.LastData item in Model)
                    {
                        if (tag)
                        {
                    <li class="Track_message_top">
                        <em>
                            <img src="/Areas/MShop/Themes/M1/Content/images/111_03.png" /></em>
                        <div class="">
                            <p>@item.context</p>
                            <span style="color: #333333;">@item.time</span>
                        </div>
                    </li>
                            tag = false;
                        }
                        else
                        {
                    <li>
                        <i></i>
                        <div class="">
                            <p>@item.context</p>
                            <span>@item.time</span>
                        </div>
                    </li>
                        }
                    }
                }
            </ul>
        </div>
    </div>
</div>
