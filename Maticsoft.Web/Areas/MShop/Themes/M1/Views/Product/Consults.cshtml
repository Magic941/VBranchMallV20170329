<div  id="ConsultListDiv">                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               
<div class="top"> <a href="javascript:;" onclick="history.go(-1)"><span>&lt;返回</span></a>
  @Html.Partial("/Areas/MShop/Themes/M1/Views/Partial/_HeadRight.cshtml")
</div>
@*<div class="productshow_title">@ViewBag.ProductName</div>*@
<div class="list">
    @if (Model != null)
    {
        if(Model.Count>0)
        {
        <div class="mt">规格参数</div>
        <div class=" consult">
            @if (Model == null) { return; }
            <table width="100%" cellpadding="5" cellspacing="0" border="1" style="border-collapse: collapse; border: 1px solid #ccc">
                <th colspan="2" style="text-align:center; font-weight:bold">主体</th>
                @foreach (Maticsoft.Model.Shop.Products.AttributeInfo item in Model)
                {
                    if (item.AttributeValues != null || item.AttributeValues.Count > 0)
                    {
                        {
                            string xx = string.Join(" ", item.ValueStr);
                    <tr style="border: 1px solid #ccc">
                        <td class="tb_lab">@item.AttributeName &nbsp;</td>
                        <td>&nbsp;&nbsp;@xx</td>
                    </tr>
                        }
                    }
                }
            </table>
        </div>
        }
        else{
            <div class="mt">规格参数</div>
            <table width="100%" cellpadding="5" cellspacing="0" border="1" style="border-collapse: collapse; border: 1px solid #ccc">
                <th colspan="2" style="text-align:center; font-weight:bold">主体</th>
                 <tr style="border: 1px solid #ccc">
                      <td colspan="2" class="tb_lab" style="text-align:center">该商品暂未设置任何参数</td>
                  </tr>
            </table>
            <div style="margin-bottom:220px;">&nbsp;</div>
        }
    }
    else
    {
    <div class="mt">规格参数</div>
    <div style="width:100%; margin:0 auto ; text-align:center; margin-top:10px;">
        <label style="text-align:center; font-size:16px;">
            该商品暂未设置任何参数
        </label>
    </div>
    <div style="margin-bottom:220px;">&nbsp;</div>
    }
</div>
</div>
