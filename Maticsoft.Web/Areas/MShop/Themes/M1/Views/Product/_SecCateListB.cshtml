@model List<Maticsoft.Model.Shop.Products.CategoryInfo>
           
@if (Model != null && Model.Count > 0)
  {
         for (int i = 0; i < Model.Count; i++)
          {
              <li>
                
                    <span>@Model[i].Name</span>
                    <div class="">
                          @Html.Action("CategoryListThree", "Product", new { parentId = Model[i].CategoryId, viewName = "_SecCateListC" })     
                    </div>
                </li>
           }
  
  }
