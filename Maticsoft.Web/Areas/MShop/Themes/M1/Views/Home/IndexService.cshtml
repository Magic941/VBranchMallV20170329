<style>
    .Wechat_a {
        width: 100%;
        height: 3rem;
        background: #fff;
        margin-bottom: 10px;
    }

        .Wechat_a a {
            width: 50%;
            height: 3rem;
            line-height: 3rem;
            display: inline-block;
            *display: inline;
            zoom: 1;
            float: left;
            text-align: center;
            border-bottom: 2px solid #cbcbcb;
        }

            .Wechat_a a.ccc {
                border-bottom: 2px solid #e7114e;
            }

    .Wechat_b {
        width: 90%;
        margin: 0 auto;
        border: 1px solid #d3d3d3;
        background: #fff;
    }

    .Wechat_bag {
        border-bottom: 1px solid #d3d3d3;
    }

        .Wechat_bag a {
            width: 95%;
            margin: 0 auto;
            display: block;
        }

            .Wechat_bag a img {
                width: 100%;
                padding: 0.4rem 0 0.1rem 0;
            }

    .Wechat_b p {
        width: 95%;
        margin: 0 auto;
        font-size: 1rem;
    }

        .Wechat_b p a {
            color: #7f7f7f;
        }

    .Wechat_b em {
        font-style: normal;
        width: 95%;
        margin: 0 auto;
        color: #d03b3b;
        font-size: 1.5rem;
        display: block;
    }

    .Wechat_cn {
        width: 90%;
        margin: 0 auto;
    }

        .Wechat_cn ul {
            overflow: hidden;
            padding: 0;
        }

        .Wechat_cn li {
            float: left;
            width: 48%;
            border: 1px solid #d3d3d3;
            background: #fff;
            margin-bottom: 0.5rem;
        }

            .Wechat_cn li a img {
                border-bottom: 1px solid #d3d3d3;
            }

    .Wechat_baf a img {
        width: 100%;
    }

    .Wechat_cn li:nth-child(2n+1) {
        float: right;
    }

    .Wechat_cn li p {
        width: 90%;
        margin: 0 auto;
        color: #3d3d3d;
        font-size: 1rem;
    }

        .Wechat_cn li p a {
            color: #3d3d3d;
        }

    .Wechat_sell {
        width: 90%;
        overflow: hidden;
        margin: 0 auto;
    }

        .Wechat_sell em {
            font-style: normal;
            color: #d03b3b;
            font-size: 1rem;
            float: left;
            padding-right: 5px;
        }

        .Wechat_sell i {
            font-style: normal;
            color: #999999;
            font-size: 1rem;
            float: left;
        }

        .Wechat_sell a {
            width: 1.8rem;
            height: 1.8rem;
            text-align: right;
            display: block;
            float: right;
        }

            .Wechat_sell a img {
                width: 100%;
            }
</style>
