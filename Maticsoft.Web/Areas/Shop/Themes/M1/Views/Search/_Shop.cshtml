@model  Maticsoft.Model.Shop.Supplier.SupplierInfo
  <!--搜索店铺-->
    @if (Model != null)
    {
     <div id="flagship-store" class="m" style="">
         <div class="store-logo">
             <a href="@(ViewBag.BasePath)store/@Model.SupplierId"  target="_blank">
                 <img width="180px" height="60px" src="@(ViewBag.BasePath)Partial/SuppLogo?id=@(Model.SupplierId)&size=T180X60" onerror="this.onerror='';this.src='/Content/themes/base/images/404/404_180X60.jpg'" class="err-product">
             </a>
         </div>
         <div class="store-info"><h2><a style="    font-weight: bold;color: #333;" href="@(ViewBag.BasePath)store/@Model.SupplierId" title="@Model.ShopName"  target="_blank">@Model.ShopName</a></h2><div class="shop-about"> </div></div>
            <div class="store-number" style="display:none;"><dl>
                                          <dd id="evaluate" title="9.6280分"><em>服务评价：</em><span class="grade"><em style="left:-4px"></em></span> @Model.ScoreService.ToString("0.0") 分</dd>
                                      </dl></div>
                                      <a href="@(ViewBag.BasePath)store/@Model.SupplierId" class="go-store"  target="_blank">进入店铺</a>
        </div>
    }   