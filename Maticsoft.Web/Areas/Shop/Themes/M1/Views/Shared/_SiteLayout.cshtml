<!DOCTYPE html>
<html>
<head>
 
    @Html.MaticsoftSEO(PoweredBy: true)
    <script src="/Scripts/jquery-1.8.3.min.js" type="text/javascript"></script>
    <script src="/Scripts/jquery/maticsoft.jquery.min.js" type="text/javascript"></script>
    <link href="@Url.Content("~/Scripts/msgbox/css/msgbox.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/msgbox/js/msgbox.js")" type="text/javascript"></script>
    <script type="text/javascript">var $Maticsoft = { BasePath: '@(ViewBag.BasePath)', CurrentArea: '@(ViewBag.CurrentArea)' };</script>
    @*------------以下是M1模版的css或者用到的js ---------*@
    @RenderSection("headtop", false)
    <link href="/Areas/Shop/Themes/M1/Content/css/g-v1-a.css" rel="stylesheet" type="text/css" />
    <link href="/Areas/Shop/Themes/M1/Content/Css/css/base.css" rel="stylesheet" />
    <link href="/Areas/Shop/Themes/M1/Content/Css/css/main-catg.css" rel="stylesheet" />
    <link href="/Areas/Shop/Themes/M1/Content/Css/css/index/marquee.css" rel="stylesheet" />
    <script src="/Areas/Shop/Themes/M1/Content/Scripts/base/base.js"></script>
    <script src="/Areas/Shop/Themes/M1/Content/Scripts/index/index.js"></script>
    <script src="/Scripts/jquery/jquery.lazyload.js?ver=@DateTime.Now.ToString("yyyyMMdd")" type="text/javascript"></script>  
     <script src="http://apps.bdimg.com/libs/angular.js/1.4.6/angular.min.js"></script> 
    <script>
        $(function () {
            $(".main_categary").mouseover(function () {
                $(".new_show_sort2").hide();
                $(this).find(".new_show_sort2").show();
                $(this).find(".item-list").show();

            });

            $(".main_categary").mouseout(function () {
                $(this).find(".item-list").hide();
                $(".new_show_sort2").hide();
            });

            $("#tabA,#tabB").mouseover(function () {
                $("#tabA,#tabB").attr("class", "");
                $(this).attr("class", "active");

                if ($(this).attr("id") == "tabA") {
                    $(".best_seller").show();
                    $(".Business_logo").hide();
                }
                if ($(this).attr("id") == "tabB") {
                    $(".best_seller").hide();
                    $(".Business_logo").show();
                }
            });

            $("#tab1,#tab2,#tab3,#tab4,#tab5,#tab6").mouseover(function () {
                $(".classifi_NEW").find("a").attr("class", "");
                $(this).attr("class", "active");
                $(".product_NEW2").hide();
                $("#" + $(this).attr("id") + "1").show();
            });
        });
    </script>
    @RenderSection("head", false)
</head>
<body>
    <div class="LoadingImg" style="display: none"></div>
    @RenderBody()
    @RenderSection("foot", false)
</body>
</html>
