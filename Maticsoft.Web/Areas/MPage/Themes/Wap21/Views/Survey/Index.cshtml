<!--在线调查开始 <div id="survey" class="page ui-page ui-body-c ui-page-active" title="在线调查" data-role="page" ax-type="ajax" data-url="survey" tabindex="0" style="min-height: 847px;">-->
@model List<Maticsoft.Model.Poll.Topics>
<header class="header view">
<span class="nowposition">
<a href="javascript:;" onclick="pageload('Home','index','首页','_Index', '_Index');" class="ui-link">首页</a>
<a href="javascript:;"   class="ui-link">在线调查</a>
</span>
</header>
<div class="content content_list">
<div class="view_remark">
    <form  >
           @if (Model != null)
           {
               for (int i=0 ;i<Model.Count;i++)
               { 
                   <fieldset>
                       <legend>@(i+1) @Model[i].Title</legend>
                       @Html.Action("Options", "Survey", new {qnumber=i+1, topicid = Model[i].ID, viewName = "_Options", type = Model[i].Type })          
                   </fieldset>              
               }
           } 
                <div style="display: inline-block; width: 100%;margin-left: 20px;">
                           <input type="button" name="add_btn" class="button" data-role="none" value=" 投票 " onclick="submitOptions();">
                           <input type="reset" name="clear_btn" value=" 重新填写 " class="button" data-role="none">
                      
                <a class="button ui-link"  onclick="pageload('Survey','result','查看结果','Result', 'Result','-1','-1','-1','-1','-1','@(ViewBag.FormID)');"> 查看结果 </a>
                </div>  
            
     </form>
</div>
</div>
<div class="pagePrevNext controlgroup" style="display:none;">
<span>
<a   class="ui-link">下载中心</a>
</span>
<span>
<a   class="ui-link">网站首页</a>
</span>
</div>
 

<!--</div>在线调查结束-->
 