@using Maticsoft.Model;
@{
    Layout = "/Areas/CMS/Themes/M1/Views/Shared/_JKIIBaseLayout.cshtml";
}
@model Shop_CardUserInfo
<link rel="stylesheet" type="text/css" href="/Areas/CMS/Themes/M1/Content/99jkc/css/about.css" />

<div class="main">
    <div class="wrapper">
        <div class="imgbox clearfix">
            <div class="lft">
                <img src="/Areas/CMS/Themes/M1/Content/99jkc/images/img_01.png" alt="">
            </div>
            <div class="rgt">
                <img src="/Areas/CMS/Themes/M1/Content/99jkc/images/img_02.png" alt="">
            </div>
        </div>
        <div class="mainBox">
            <div class="leftBox">
                <ul class="subNav">
                    <li class="on"><a href="#">查询信息<i></i></a></li>
                </ul>
            </div>
            <!--leftbox-->
            <div class="rightBox">
                <div class="position">您所在的位置：<a href="/">主页</a> &gt; <a href="/">账号激活</a> &gt; <a href="/">查询信息</a></div>
                <div class="body">
                    <div class="fuwu" style="margin-top: 20px;">
                        <!--服务凭证开始-->
                        <div class="fwpz1">卡激活信息查询</div>
                        @if (Model != null)
                        {
                            <p>投保人信息</p>
                            <table width="100%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#666" class="fwpz24">
                                <tbody>
                                    <tr>
                                        <td bgcolor="#FFFFFF">
                                            <table width="100%" border="0" cellpadding="0" cellspacing="0" class="fwpz21">
                                                <tbody>
                                                    <tr>
                                                        <td width="28%">卡号：@Model.CardNo</td>
                                                        <td class="fwpz22"></td>
                                                        <td width="28%">激活日期：@Model.ActiveDate.ToString("yyyy-MM-dd hh:mm:ss")</td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td>投保人：@Model.Name</td>
                                                        <td class="fwpz22">&nbsp;</td>
                                                        <td>身份证号：@Model.CardId</td>
                                                        <td>&nbsp;</td>
                                                    </tr>
                                                    <tr>
                                                        <td>出生日期：@Model.BirthDay.ToString("yyyy-MM-dd")</td>
                                                        <td class="fwpz22">&nbsp;</td>
                                                        <td>性别:@Model.SexTxt</td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td>手机号：@Model.Moble</td>
                                                        <td class="fwpz22">&nbsp;</td>
                                                        <td>地址：@Model.Address</td>
                                                        <td></td>
                                                    </tr>
                                                    <tr>
                                                        <td>紧急联系人：@Model.BackPerson</td>
                                                        <td class="fwpz22">&nbsp;</td>
                                                        <td>报案电话：@Model.TelephoneRreport</td>
                                                        <td></td>
                                                    </tr>

                                                    @if (!string.IsNullOrEmpty(Model.InsureNo))
                                                    {
                                                       <tr>
                                                        <td>保单号：@Model.InsureNo</td>
                                                        <td class="fwpz22">&nbsp;</td>
                                                        <td>保单生效日期：
                                                            @if (Model.InsureOrderStart>= Convert.ToDateTime("1900-01-02"))
                                                            {
                                                                 <p>@Model.InsureOrderStart.ToString("yyyy-MM-dd")</p>
                                                            }
                                                            else
                                                            {
                                                                <p>--</p>
                                                            }

                                                        </td>
                                                        <td>保单到期日：
                                                            @if (Model.InsureOrderEnd>= Convert.ToDateTime("1900-01-02"))
                                                            {
                                                                 <p>@Model.InsureOrderEnd.ToString("yyyy-MM-dd")</p>
                                                            }
                                                            else
                                                            {
                                                                 <p>--</p>
                                                            }

                                                        </td>
                                                    </tr>
                                                    }

                                                  @*  @if (!string.IsNullOrEmpty(Model.InsureNo))
                                                    {
                                                        <tr>
                                                            <td>保单号：@Model.InsureNo</td>
                                                            <td class="fwpz22">&nbsp;</td>
                                                            <td>保单文件：<a href="@Model.InsureOrderFileUrl" target="_blank">点击查看保单</a></td>
                                                            <td></td>
                                                        </tr>
                                                    }*@
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>

                                </tbody>
                            </table>                            
                                                    for (int i = 1; i <= Model.CardInsurantList.Count; i++)
                                                    {
                                                        var item = Model.CardInsurantList[i - 1];
                                                        <p>被保人@(i)信息</p>
                                                        <table width="100%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#666" class="fwpz24">
                                                            <tbody>
                                                                <tr>
                                                                    <td bgcolor="#FFFFFF">
                                                                        <table width="100%" border="0" cellpadding="0" cellspacing="0" class="fwpz21">
                                                                            <tbody>
                                                                                <tr>
                                                                                    <td width="28%">姓名：@item.Name</td>
                                                                                    <td class="fwpz22"></td>
                                                                                    <td width="28%">和投保人关系：@item.RelationShipTxt</td>
                                                                                    <td></td>
                                                                                </tr>
                                                                                <tr>
                                                                                    <td>证件类型：@item.CertificateTypeTxt</td>
                                                                                    <td class="fwpz22">&nbsp;</td>
                                                                                    <td>证件号：@item.CardId</td>
                                                                                    <td>&nbsp;</td>
                                                                                </tr>
                                                                                <tr>
                                                                                    <td>出生日期：@item.BirthDay.ToString("yyyy-MM-dd");</td>
                                                                                    <td class="fwpz22">&nbsp;</td>
                                                                                    <td>性别：@item.SexTxt</td>
                                                                                    <td></td>
                                                                                </tr>
                                                                                 <tr>
                                                                                    <td colspan="3">职业类型：@item.JobType</td>                                                     
                                                                                </tr>
                                                                            </tbody>
                                                                        </table>
                                                                    </td>
                                                                </tr>

                                                            </tbody>
                                                        </table>   
                                                    }

                        }else{
                        <span>未查到激活信息</span>
                        }
                    </div>
                </div>

                <!--服务凭证结束-->
            </div>
            <!--rightbox-->
            <div class="clear"></div>
        </div>
    </div>
</div>
